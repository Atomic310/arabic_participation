<%= stylesheet_link_tag 'logs.css' %>
<p id="notice"><%= notice %></p>

<body class="log">
<h1>Logs</h1>

<table id="myTable2">
  <thead>
    <tr>
      <th><%= link_to "Log", :sort => "log_id"%> </th>
      <th><%= link_to "UIN", :sort => "uin"%></th>
      <th><%= link_to "Event", :sort => "event_id"%></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.log_id %></td>
        <td><%= log.uin %></td>
        <td><%= log.event_id %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Edit', edit_log_path(log) %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log', new_log_path %>




</body>
